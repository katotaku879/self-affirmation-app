[{"quote": "\u4eca\u65e5\u306e\u3042\u306a\u305f\u306f\u3001\u6628\u65e5\u306e\u3042\u306a\u305f\u304c\u61a7\u308c\u305f\u59ff\u3067\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u7d99\u7d9a\u306f\u529b\u306a\u308a\u3002\u6bce\u65e5\u306e\u5c0f\u3055\u306a\u4e00\u6b69\u304c\u3001\u5927\u304d\u306a\u5909\u5316\u3092\u751f\u307f\u51fa\u3057\u307e\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u30b4\u30fc\u30eb\u3092\u898b\u5931\u3063\u305f\u3068\u304d\u3001\u30d7\u30ed\u30bb\u30b9\u3092\u4fe1\u3058\u307e\u3057\u3087\u3046\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u5b8c\u74a7\u3092\u76ee\u6307\u3059\u3088\u308a\u3082\u3001\u524d\u9032\u3057\u7d9a\u3051\u308b\u3053\u3068\u304c\u5927\u5207\u3067\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u81ea\u5206\u3092\u4fe1\u3058\u308c\u3070\u3001\u4f55\u3067\u3082\u3067\u304d\u308b\u3002\u305f\u3060\u3001\u52aa\u529b\u306f\u5fc5\u8981\u3067\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u4e00\u65e5\u4e00\u65e5\u304c\u6210\u9577\u306e\u6a5f\u4f1a\u3067\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u4eca\u65e5\u4e00\u65e5\u3001\u3042\u306a\u305f\u304c\u3069\u3093\u306a\u9078\u629e\u3092\u3059\u308b\u304b\u3067\u672a\u6765\u304c\u5909\u308f\u308a\u307e\u3059\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u81ea\u5206\u3092\u6279\u5224\u3059\u308b\u3088\u308a\u3082\u3001\u81ea\u5206\u3092\u52b1\u307e\u3057\u307e\u3057\u3087\u3046\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u5c0f\u3055\u306a\u9032\u6b69\u3082\u3001\u9032\u6b69\u3067\u3059\u3002\u81ea\u5206\u306e\u6210\u9577\u3092\u795d\u3044\u307e\u3057\u3087\u3046\u3002", "author": "\u4e0d\u660e"}, {"quote": "\u4e00\u5ea6\u306e\u5931\u6557\u306f\u6210\u529f\u3078\u306e\u4e00\u6b69\u3067\u3059\u3002\u8ae6\u3081\u306a\u3044\u3067\u304f\u3060\u3055\u3044\u3002", "author": "\u4e0d\u660e"}]